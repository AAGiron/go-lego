  "version": "0.2.0",
  "configurations": [
    {
      "name": "ACME new challenge: classical certificate",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceRoot}/cmd/lego/main.go",
      "dlvFlags": ["--check-go-version=false"],
      "args": [
        "-s", "https://localhost:14000/dir",
        "-d", "teste",
        "-m", "teste@teste.com",
        "--http.port", ":5002",
        "--http", 
        "-a",
        "--pqtls",
        "--kex", "Kyber512",
        "-k", "ec384",
        "--certalgo", "P384",
        "--pqtls",
        "run"
      ],
    },
    {
      "name": "ACME new challenge: post-quantum certificate",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "cwd": "${workspaceRoot}",
      "program": "${workspaceRoot}/cmd/lego/main.go",
      "dlvFlags": ["--check-go-version=false"],
      "args": [
        "-s", "https://localhost:14000/dir", 
        "-d", "teste", 
        "-m", "teste-newchallenge@teste.com", 
        "--http.port", ":5002", 
        "--http",
        "-a",
        "-newchallenge",
        "-pqorderport", "10003",
        "--pqtls",
        "--kex", "Kyber512", 
        "-k", "Dilithium2",
        "--certalgo", "Dilithium2",
        "run"
      ],
    }
  ]
}